<resources>

    <style name="CSLayout">
        <item name="android:ellipsize">end</item>
    </style>

    <style name="CSMatchWrap" parent="CSLayout">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSWrapMatch" parent="CSLayout">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSMatch" parent="CSLayout">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSWrap" parent="CSLayout">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSWrapFill" parent="CSWrap">
        <item name="android:gravity">center</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSMatchFill" parent="CSLayout">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSFillMatch" parent="CSLayout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSFillWrap" parent="CSLayout">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSFill">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSFillTiny" parent="CSFill">
        <item name="android:layout_weight">0.05</item>
    </style>

    <style name="CSFillTinyMatch" parent="CSFillTiny">
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSFillShort" parent="CSFill">
        <item name="android:layout_weight">0.1</item>
    </style>

    <style name="CSFillShortMatch" parent="CSFillShort">
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSFillMedium" parent="CSFill">
        <item name="android:layout_weight">0.3</item>
    </style>

    <style name="CSFillMediumMatch" parent="CSFillMedium">
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSFillLarge" parent="CSFill">
        <item name="android:layout_weight">0.6</item>
    </style>

    <style name="CSFillLargeMatch" parent="CSFillLarge">
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSScrollMatch" parent="CSMatch">
        <item name="android:fillViewport">true</item>
    </style>

    <style name="CSScrollMatchWrap" parent="CSScrollMatch">
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSScrollMatchFill" parent="CSMatchFill">
        <item name="android:fillViewport">true</item>
    </style>

    <style name="CSContainer">
        <item name="android:background">?android:attr/colorBackground</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
    </style>

    <style name="CSNavigationContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:clickable">false</item>
    </style>


    <style name="CSVerticalMatchWrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="CSVerticalMatchFill">
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="CSVerticalWrapFill" parent="CSVerticalMatchFill">
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="CSVerticalFillWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="CSVerticalFillMatch" parent="CSVerticalFillWrap">
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSVerticalWrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="CSVerticalWrapMatch" parent="CSVerticalWrap">
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSVerticalMatch">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="CSHorizontalWrap">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="CSHorizontalWrapFill">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="CSHorizontalMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="CSHorizontalMatchWrap">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="CSHorizontalWrapMatch">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="CSHorizontalFillWrap" parent="CSHorizontalMatchWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSHorizontalFillMatch" parent="CSHorizontalMatchWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSHorizontalMatchFill" parent="CSHorizontalMatch">
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>


    <!--    <style name="CSContentMain" parent="CSMatchFill" />-->

    <style name="CSGridMatch" parent="android:Widget.Material.GridView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:horizontalSpacing">0dp</item>
        <item name="android:verticalSpacing">0dp</item>
        <item name="android:numColumns">auto_fit</item>
        <item name="android:scrollbars">none</item>
        <item name="android:stretchMode">columnWidth</item>
        <item name="android:choiceMode">none</item>
        <item name="android:listSelector">@color/cs_transparent</item>
    </style>

    <style name="CSGridFillMatch" parent="CSGridMatch">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSGridMatchFill" parent="CSGridMatch">
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

