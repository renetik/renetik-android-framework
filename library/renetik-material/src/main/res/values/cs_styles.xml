<resources>

    <style name="CSLayout">
        <item name="android:ellipsize">end</item>
    </style>

    <style name="CSMatchWrap" parent="CSLayout">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSWrapMatch" parent="CSLayout">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSMatch" parent="CSLayout">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSWrap" parent="CSLayout">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSWrapFill" parent="CSWrap">
        <item name="android:gravity">center</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSMatchFill" parent="CSLayout">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSFillMatch" parent="CSLayout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSFillWrap" parent="CSLayout">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSContainer">
        <item name="android:background">?attr/colorSurface</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:descendantFocusability">beforeDescendants</item>
        <item name="android:focusableInTouchMode">true</item>
    </style>

    <style name="CSNavigationContainer">
        <item name="android:background">?attr/colorSurface</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:clickable">false</item>
    </style>

    <style name="CSContentEmpty" parent="CSWrap">
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textAppearance">@style/CSTextOverline</item>
    </style>

    <style name="CSVerticalLayoutMatchWrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="CSVerticalLayoutMatchFill">
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSVerticalLayoutFillWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSVerticalLayoutWrap" parent="CSWrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="CSVerticalLayoutMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="CSHorizontalLayoutMatchWrap">
        <item name="android:orientation">horizontal</item>
        <item name="android:baselineAligned">false</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSHorizontalLayoutWrapMatch">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSHorizontalLayoutMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="CSHorizontalLayoutMatchFill">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="CSHorizontalLayoutWrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="CSPageTitleCard" parent="Widget.MaterialComponents.CardView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSPageTitleMatchWrap" parent="CSMatchWrap">
        <item name="android:gravity">center</item>
        <item name="android:padding">3sp</item>
        <item name="android:textAppearance">@style/TextAppearance.MaterialComponents.Subtitle2
        </item>
    </style>

    <style name="CSTabStripBottom" parent="CSMatch">
        <item name="android:background">?attr/colorPrimaryVariant</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:textAppearance">@style/CSTextSubtitle</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="CSTabStripTop" parent="CSTabStripBottom">
        <item name="android:layout_gravity">top</item>
    </style>

    <style name="CSContentCardWrap" parent="Widget.MaterialComponents.CardView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>

    <style name="CSContentCardMatchWrap" parent="CSContentCardWrap">
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="CSContentCardMatchFill" parent="CSContentCardMatchWrap">
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSContentMain" parent="CSMatchFill" />

    <style name="CSGridMatch" parent="android:Widget.Material.GridView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:horizontalSpacing">0dp</item>
        <item name="android:verticalSpacing">0dp</item>
        <item name="android:numColumns">auto_fit</item>
        <item name="android:scrollbars">none</item>
        <item name="android:stretchMode">columnWidth</item>
    </style>

    <style name="CSGridFillMatch" parent="CSGridMatch">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSListMatch" parent="android:Widget.Material.ListView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CSListPullMatch" parent="CSMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/cs_transparent</item>
        <item name="android:padding">2dp</item>
    </style>

    <style name="CSListItem" parent="CSHorizontalLayoutMatchWrap">
        <item name="android:padding">3dp</item>
        <item name="android:background">?android:attr/activatedBackgroundIndicator</item>
        <item name="android:layout_marginEnd">3dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>

    <style name="CSGridItemCardMatchWrap" parent="Widget.MaterialComponents.CardView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_margin">1dp</item>
    </style>

    <style name="CSGridItemMatchWrap" parent="CSMatchWrap">
        <item name="android:background">?android:attr/activatedBackgroundIndicator</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">2dp</item>
    </style>

    <style name="CSButtonMatchWrap" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSButtonWrap" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="CSButtonFillWrap" parent="CSButtonMatchWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSButtonVariantMatchWrap" parent="CSButtonMatchWrap">
        <item name="backgroundTint">?attr/colorPrimaryVariant</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="CSButtonVariantFillWrap" parent="CSButtonVariantMatchWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSButtonTextMatchWrap" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSButtonTextWrap" parent="CSButtonTextMatchWrap">
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="CSButtonTextFillWrap" parent="CSButtonTextMatchWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSButtonFloatingTopEndWrap" parent="Widget.MaterialComponents.FloatingActionButton">
        <item name="android:layout_gravity">top|end</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:layout_marginStart">0dp</item>
        <item name="android:layout_marginBottom">0dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSButtonFloatingTopStartWrap" parent="CSButtonFloatingTopEndWrap">
        <item name="android:layout_gravity">top|start</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginBottom">0dp</item>
        <item name="android:layout_marginEnd">0dp</item>
    </style>

    <style name="CSButtonFloatingBottomEndWrap" parent="CSButtonFloatingTopEndWrap">
        <item name="android:layout_gravity">bottom|end</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginStart">0dp</item>
    </style>

    <style name="CSButtonFloatingBottomStartWrap" parent="CSButtonFloatingBottomEndWrap">
        <item name="android:layout_gravity">bottom|start</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginEnd">0dp</item>
    </style>

    <style name="CSButtonIconWrap" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
    </style>

    <style name="CSInputLayoutMatchWrap" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSInputLayoutDropdownMatchWrap" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSInputLayoutFillWrap" parent="CSInputLayoutMatchWrap">
        <item name="android:layout_gravity">center|top</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSInputEditText" parent="Widget.MaterialComponents.TextInputEditText.OutlinedBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="CSInputEditTextClickable" parent="CSInputEditText">
        <item name="android:inputType">textFilter|textMultiLine|textNoSuggestions</item>
        <item name="android:cursorVisible">false</item>
        <item name="android:focusable">false</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:foreground">?android:attr/selectableItemBackgroundBorderless</item>
    </style>

    <style name="CSContainerEmptyText" parent="CSWrap">
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textAppearance">@style/CSTextOverline</item>
    </style>

    <style name="CSActionBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:contentInsetStart">0dp</item>
        <item name="android:contentInsetLeft">0dp</item>
        <item name="android:contentInsetEnd">0dp</item>
        <item name="android:contentInsetRight">0dp</item>
        <item name="android:elevation">4dp</item>
        <item name="contentInsetEnd">0dp</item>
        <item name="contentInsetLeft">0dp</item>
        <item name="contentInsetRight">0dp</item>
        <item name="contentInsetStart">0dp</item>
    </style>

    <style name="CSBorderLeft">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:background">?attr/colorPrimaryVariant</item>
    </style>

    <style name="CSBorderRight">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:background">?attr/colorPrimaryVariant</item>
    </style>

    <style name="CSSpaceHorizontal3dp">
        <item name="android:layout_width">3dp</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="CSSpaceVertical3dp">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">3dp</item>
    </style>

    <style name="CSSpaceHorizontal5dp">
        <item name="android:layout_width">5dp</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="CSSpaceVertical5dp">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">5dp</item>
    </style>

</resources>
